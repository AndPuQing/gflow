{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "silent": true
  },
  "ignoreBuildStep": {
    "command": "bash -c '[[ $(git diff HEAD^ HEAD --name-only | grep -E \"^docs/\") ]]'"
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ]
}
